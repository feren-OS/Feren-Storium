{
    "disable-googlesignon": {
        "vivaldi": {
            "privacy": {
                "google_component_extensions": {
                    "crypto_token": false
                }
            }
        }
    },
    "disable-googlehangouts": {
        "brave": {
            "hangouts_enabled": false
        },
        "vivaldi": {
            "privacy": {
                "google_component_extensions": {
                    "hangout_services": false
                }
            }
        }
    },
    "disable-history": {
        "browser": {
            "show_toolbar_history_button": false
        },
        "vivaldi": {
            "panels": {
                "list": [
                    { "available": false, "forMailOnly": false, "id": "history", "resizable": false, "width": -1 }
                ]
            },
            "tabs": {
                "show_trash_can": false
            },
            "toolbars": { "navigation": ["AddressField", "DownloadButton", "Extensions", "UpdateButton"] }
        }
    }

}

{
    "managed-via-gui": false,
    "can-be-toggled": false,
    "hide-unavailable-sources": true,
    "name": "TODO: Remove the above, moving them but name to module's own APIgetAllAvailableSources return response",
    "browsers": {
        "vivaldi": {
            "name": "Vivaldi",
            "command": ["/usr/bin/vivaldi", "%WEBSITEURL%", "--class=%WINCLASS%", "--user-data-dir=%PROFILEDIR%"],
            "required-file": ["/usr/bin/vivaldi"],
            "pkgidlink": ["vivaldi-vivaldi-webbrowser"],
            "classprefix": "vivaldistoriumice?",
            "type": "chromium",
            "flatpak": ""
        },
        "chrome": {
            "name": "Google Chrome",
            "command": ["/usr/bin/google-chrome", "--app=%WEBSITEURL%", "--class=%WINCLASS%", "--user-data-dir=%PROFILEDIR%"],
            "required-file": ["/usr/bin/google-chrome"],
            "pkgidlink": ["google-chrome-webbrowser"],
            "classprefix": "",
            "type": "chromium",
            "flatpak": ""
        },
        "msedge": {
            "name": "Microsoft Edge",
            "command": ["/usr/bin/microsoft-edge", "--app=%WEBSITEURL%", "--class=%WINCLASS%", "--user-data-dir=%PROFILEDIR%"],
            "required-file": ["/usr/bin/microsoft-edge"],
            "pkgidlink": ["microsoft-edge-webbrowser"],
            "classprefix": "",
            "type": "chromium",
            "flatpak": ""
        },
        "brave": {
            "name": "Brave",
            "command": ["/usr/bin/brave-browser", "--app=%WEBSITEURL%", "--class=%WINCLASS%", "--user-data-dir=%PROFILEDIR%"],
            "required-file": ["/usr/bin/brave-browser"],
            "pkgidlink": ["brave-brave-webbrowser"],
            "classprefix": "",
            "type": "chromium",
            "flatpak": ""
        },
        "ungoogled-chromium": {
            "name": "Ungoogled Chromium",
            "command": ["flatpak run com.github.Eloston.UngoogledChromium", "--app=%WEBSITEURL%", "--class=%WINCLASS%", "--user-data-dir=%PROFILEDIR%"],
            "required-file": ["/var/lib/flatpak/app/com.github.Eloston.UngoogledChromium/current/active/export/bin/com.github.Eloston.UngoogledChromium"],
            "pkgidlink": ["ungoogled-chromium-webbrowser"],
            "classprefix": "",
            "type": "chromium",
            "flatpak": "com.github.Eloston.UngoogledChromium"
        }
    }
}
